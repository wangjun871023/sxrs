<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="user_table" catalog="sxrs" class="UserEntity"></table>
	<table name="dept_table" catalog="sxrs" class="DeptEntity">
		<primary-key>
			<generator class="uuid2"></generator>
		</primary-key>
	</table>
	<table name="menu_table" catalog="sxrs"
		class="com.sxrs.pojo.MenuEntity">
		<primary-key>
			<generator class="uuid2"></generator>
		</primary-key>
	</table>
<table name="model_table" catalog="sxrs"
	class="com.sxrs.pojo.ModelEntity">
	<primary-key>
		<generator class="uuid2"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>